                                                                            <div class="IdImage">
                                                                                @if (Model.ApplicationUser.IdImage != null)
                                                                                {
                                                                                    @Html.Image("/Uploads/ProfileImages/" + @User.Identity.GetUserId() + "/" + @Model.ApplicationUser.IdImage)
                                                                                    <span class="btn remove-idimage">X</span>
                                                                                }
                                                                            </div>

                                                                            <div style="margin-top: 20px;"><h4 class="text-center dropzone-error2 text-light small"></h4></div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>
                                                    <div class="tab-pane fade" id="payment" role="tabpanel" aria-labelledby="payment-tab">

                                                        test 3
                                                    </div>
                                                </div>



                                            </div>
                                            <div class="card-footer text-right">
                                                <button class="btn btn-primary submit" type="submit">Update Profile</button>
                                            </div>
