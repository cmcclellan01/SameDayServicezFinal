<script>




    $('.accept-applicant').on('click', function (evt) {
        var element = $(this);
        swal({
            title: "Accept Applicant?",
            text: $('.accept').html(),
            type: "info",
            showCancelButton: true,
            confirmButtonClass: "btn-info",
            confirmButtonText: "Accept",
            closeOnConfirm: true,
            html: true
        },
            function () {
                $.ajax({
                    type: "POST",
                    url: '/Account/AssignContractor?ApplicantId=' + element.attr('data-applicantId') + '&projectId=' + element.attr('data-projectId'),
                    success: function () {
                        $.get('/Account/GetProjectAssignmentSingle?ProjectsId=' + element.attr('data-projectId') + '&ApplicantId=' + element.attr('data-applicantId'), function (data) {
                            $('.assigned-contactors').append(data);
                            $('.accept-applicant-' + element.attr('data-guid')).remove();

                            if (($('.ProjectApplicants-badge').html() - 1) == 0) {
                                 $('.ProjectApplicants-badge').html('');
                            } else {
                                 $('.ProjectApplicants-badge').html($('.ProjectApplicants-badge').html() - 1);
                            }
                           
                              
                           
                        });
                    }
                });
            });
    });





    $('.reject-applicant').on('click', function (evt) {


        var element = $(this);

        if ($(this).hasClass('rejected')) {


            swal({
                title: "Rescind Applicant?",
                text: "Are you sure that you want to rescind this applicant?",
                type: "info",
                showCancelButton: true,
                confirmButtonClass: "btn-info",
                confirmButtonText: "Ok!",
                closeOnConfirm: true
            },
                function () {
                    //swal("Deleted!", "Your imaginary file has been deleted.", "success");

                    element.removeClass('rejected');
                    element.removeClass('btn-secondary');
                    element.addClass('btn-danger');
                    $.ajax({
                        type: "POST",
                        url: '/Account/RejectOrUnrejectApplicant?projectId=' + element.attr('data-projectId') + '&ApplicantId=' + element.attr('data-applicantId') + '&Reject=false',
                        success: function () {

                        }
                    });
                });

        }
        else {

            swal({
                title: "Reject Applicant?",
                text: "Are you sure that you want to reject this applicant?",
                type: "info",
                showCancelButton: true,
                confirmButtonClass: "btn-info",
                confirmButtonText: "Ok!",
                closeOnConfirm: true
            },
                function () {
                    //swal("Deleted!", "Your imaginary file has been deleted.", "success");

                    element.addClass('rejected');
                    element.addClass('btn-secondary');
                    element.removeClass('btn-danger');

                    $.ajax({
                        type: "POST",
                        url: '/Account/RejectOrUnrejectApplicant?projectId=' + element.attr('data-projectId') + '&ApplicantId=' + element.attr('data-applicantId') + '&Reject=true',
                        success: function () {

                        }
                    });
                });



        }


        return false;

    });

    $('.profile-btn').on('click', function (evt) {
        var $detailDiv = $('.full-applicant-profile-view');

        $.get('/Account/FullProfile?Id=' + $(this).attr('data-contractor-id'), function (data) {
            $detailDiv.replaceWith('<div class="full-applicant-profile-view">' + data + '</div>');
            //$detailDiv.replaceWith(data);

            $('.bd-example-modal-lg').modal('show').on('hidden.bs.modal', function (e) {

                $('.full-applicant-profile-view').html('');
            });

        });
    });
</script>